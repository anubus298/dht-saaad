{
  "builds": [
    {
      "src": "projet/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],

  
  "routes": [
    
    {
      "src": "/(.*)",
      "dest": "projet/wsgi.py"
    }
  ]
}