{
  "builds": [
    {
      "src": "projet/asgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "projet/static"
      }
    }
  ],

  
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "projet/asgi.py"
    }
  ]
}